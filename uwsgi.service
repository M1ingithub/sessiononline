[Unit]
Description=uWSGI daemon

[Service]
User=root
Group=root
ExecStart=/home/user/Desktop/sessiononline/venv/bin/uwsgi --ini /home/user/Desktop/sessiononline/core_uwsgi.ini
RuntimeDirectory=uwsgi
Restart=always
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target